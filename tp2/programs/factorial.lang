-- Factorial implementation
factorial() {
    --n :: Int;
    --fact :: Int;    
    n = 0;
    read n ;
    fact = 1;
    iterate n {
        fact = fact * n ;
        n = n - 1 ;
    }
    print fact ;
}

main() {
    factorial();
}